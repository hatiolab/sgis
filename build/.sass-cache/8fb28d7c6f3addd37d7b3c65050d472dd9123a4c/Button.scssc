    @if $include-slicer-border-radius {
        .#{$prefix}btn-focus {
            $stretch: slicer-background-stretch('btn-focus.#{$prefix}btn-#{$ui}', 'bottom');
            $frame: slicer-frame('btn-focus.#{$prefix}btn-#{$ui}', $frame-size);

            .#{$prefix}btn-#{$ui}-tl,
            .#{$prefix}btn-#{$ui}-bl,
